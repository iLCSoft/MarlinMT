 * These macros can be used in the following way:
 *
 *    #if MARLINMT_VERSION_CODE >= MARLINMT_VERSION(2,23,4)
 *       #include <newheader.h>
 *    #else
 *       #include <oldheader.h>
 *    #endif
 *
*/

#define MARLINMT_RELEASE "@MarlinMT_VERSION_MAJOR@.@MarlinMT_VERSION_MINOR@.@MarlinMT_VERSION_PATCH@"
#define MARLINMT_RELEASE_DATE "@MarlinMT_RELEASE_DATE@"
#define MARLINMT_RELEASE_TIME "@MarlinMT_RELEASE_TIME@"
#define MARLINMT_VERSION(a,b,c) (((a) << 16) + ((b) << 8) + (c))
#define MARLINMT_VERSION_CODE MARLINMT_VERSION(@MarlinMT_VERSION_MAJOR@,@MarlinMT_VERSION_MINOR@,@MarlinMT_VERSION_PATCH@)
