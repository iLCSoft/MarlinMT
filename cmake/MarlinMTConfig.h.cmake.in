#pragma once

/* Version information automatically generated by installer. */

/*
 * These macros can be used in the following way:
 *
 *    #if MARLIN_VERSION_CODE >= MARLIN_VERSION(2,23,4)
 *       #include <newheader.h>
 *    #else
 *       #include <oldheader.h>
 *    #endif
 *
*/

#define MARLIN_RELEASE "@Marlin_VERSION_MAJOR@.@Marlin_VERSION_MINOR@.@Marlin_VERSION_PATCH@"
#define MARLIN_RELEASE_DATE "@Marlin_RELEASE_DATE@"
#define MARLIN_RELEASE_TIME "@Marlin_RELEASE_TIME@"
#define MARLIN_VERSION(a,b,c) (((a) << 16) + ((b) << 8) + (c))
#define MARLIN_VERSION_CODE MARLIN_VERSION(@Marlin_VERSION_MAJOR@,@Marlin_VERSION_MINOR@,@Marlin_VERSION_PATCH@)
